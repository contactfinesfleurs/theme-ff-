{% assign wfPage = "640ec1b9bae1c5607003feec" %}
{% assign has_smart_seo_app = false %}
{% if content_for_header contains 'smart-seo' or content_for_header contains 'smartseo' %}
  {% assign has_smart_seo_app = true %}
{% endif %}
{%- render "seo" -%}
{%- render "social-meta-tags" -%}
