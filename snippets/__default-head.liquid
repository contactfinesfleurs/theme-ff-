{% assign load_font_awesome = settings.show_music %}
<meta charset="utf-8">
<meta content="width=device-width, initial-scale=1" name="viewport">

{% if request.page_type == 'index' %}
  <style>
    .background-image-index{position:fixed;left:0;top:0;right:0;bottom:0;z-index:-2;width:100%;min-height:100%}
    .bg-color{width:100vw;height:100%}
    .bg-color.enter-screen{display:block}
    .bg-color.enter-screen.desktop{display:block}
    .bg-color.enter-screen.mobile{display:none}
    .main-menu{position:absolute;left:0;right:0;bottom:auto;width:200px;margin-right:auto;margin-left:auto;display:flex;flex-direction:column;align-items:center}
    .logo-link{width:fit-content;height:auto;margin-bottom:15px;margin-top:20px;display:inline-block}
    .logo{display:block}
    .date-time-container{display:flex;flex-direction:column;align-items:center;gap:2px}
    .date,.time{font-size:12px;letter-spacing:2px;text-align:center}
    .nav-link{width:auto;margin-right:auto;margin-left:auto;color:var(--nav-link-color);font-size:14px;text-decoration:none;display:block}
    .enter-nav-link{font-family:var(--nav_font),sans-serif}
    .pages-menu{display:flex;flex-direction:column}
    .social-media-container{display:flex}
    .text-block{display:none}
    .email-popup__overlay{display:none}
    @media(max-width:767px){.bg-color.enter-screen.desktop{display:none}.bg-color.enter-screen.mobile{display:block}}
  </style>
  <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
  <script>
    (function () {
      var cssHrefs = [
        "{{ 'normalize.css' | asset_url }}",
        "{{ 'components.css' | asset_url }}",
        "{{ 'plain-jane.css' | asset_url }}",
        "{{ 'udesly-common.min.css' | asset_url }}"
      ];
      var stylesLoaded = false;

      function appendStyles() {
        if (stylesLoaded) return;
        stylesLoaded = true;
        cssHrefs.forEach(function (href) {
          var existing = document.querySelector('link[data-ff-core-style="' + href + '"]');
          if (existing) return;
          var link = document.createElement('link');
          link.rel = 'stylesheet';
          link.href = href;
          link.setAttribute('data-ff-core-style', href);
          document.head.appendChild(link);
        });
      }

      var isMobile = window.matchMedia && window.matchMedia('(max-width: 768px)').matches;
      if (!isMobile) {
        appendStyles();
        return;
      }

      var interactionEvents = ['pointerdown', 'touchstart', 'keydown', 'scroll'];

      function handleInteraction() {
        appendStyles();
        interactionEvents.forEach(function (eventName) {
          window.removeEventListener(eventName, handleInteraction);
        });
      }

      interactionEvents.forEach(function (eventName) {
        var options = eventName === 'keydown' ? { once: true } : { once: true, passive: true };
        window.addEventListener(eventName, handleInteraction, options);
      });

      window.addEventListener(
        'load',
        function () {
          if ('requestIdleCallback' in window) {
            requestIdleCallback(appendStyles, { timeout: 2500 });
          } else {
            setTimeout(appendStyles, 1200);
          }
        },
        { once: true }
      );
    })();
  </script>
  <noscript>
    <link rel="stylesheet" href="{{ 'normalize.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'components.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'plain-jane.css' | asset_url }}">
    <link rel="stylesheet" href="{{ 'udesly-common.min.css' | asset_url }}">
  </noscript>
  {% if settings.show_home_logo and settings.enter_screen_logo %}
    {% assign home_logo_size = settings.enter_screen_logo_size | default: 120 %}
    {% assign home_logo_preload_width = home_logo_size | times: 2 | at_least: 240 | at_most: 480 %}
    <link
      rel="preload"
      as="image"
      href="{{ settings.enter_screen_logo | image_url: width: home_logo_preload_width }}"
      imagesrcset="{{ settings.enter_screen_logo | image_url: width: home_logo_size }} {{ home_logo_size }}w, {{ settings.enter_screen_logo | image_url: width: home_logo_preload_width }} {{ home_logo_preload_width }}w"
      imagesizes="{{ home_logo_size }}px"
      fetchpriority="high"
    >
  {% endif %}
{% else %}
  <link rel="preconnect" href="https://cdn.shopify.com" crossorigin>
  <link rel="stylesheet" href="{{ 'normalize.css' | asset_url }}" media="print" onload="this.media='all'">
  <noscript><link rel="stylesheet" href="{{ 'normalize.css' | asset_url }}"></noscript>
  <link rel="stylesheet" href="{{ 'components.css' | asset_url }}" media="print" onload="this.media='all'">
  <noscript><link rel="stylesheet" href="{{ 'components.css' | asset_url }}"></noscript>
  <link rel="stylesheet" href="{{ 'plain-jane.css' | asset_url }}" media="print" onload="this.media='all'">
  <noscript><link rel="stylesheet" href="{{ 'plain-jane.css' | asset_url }}"></noscript>
  <link rel="stylesheet" href="{{ 'udesly-common.min.css' | asset_url }}" media="print" onload="this.media='all'">
  <noscript><link rel="stylesheet" href="{{ 'udesly-common.min.css' | asset_url }}"></noscript>
{% endif %}
{% if load_font_awesome %}
  {% if request.page_type == 'index' %}
    <script>
      (function () {
        var loaded = false;

        function loadFontAwesome() {
          if (loaded) return;
          loaded = true;
          var link = document.createElement('link');
          link.rel = 'stylesheet';
          link.href = "https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css";
          link.integrity = "sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ==";
          link.crossOrigin = 'anonymous';
          link.referrerPolicy = 'no-referrer';
          document.head.appendChild(link);
        }

        window.addEventListener('pointerdown', loadFontAwesome, { once: true, passive: true });
        window.addEventListener('touchstart', loadFontAwesome, { once: true, passive: true });
        window.addEventListener('keydown', loadFontAwesome, { once: true });
        window.addEventListener(
          'load',
          function () {
            if ('requestIdleCallback' in window) {
              requestIdleCallback(loadFontAwesome, { timeout: 3500 });
            } else {
              setTimeout(loadFontAwesome, 2000);
            }
          },
          { once: true }
        );
      })();
    </script>
    <noscript><link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></noscript>
  {% else %}
    <link rel="preconnect" href="https://cdnjs.cloudflare.com" crossorigin>
    <link rel="dns-prefetch" href="https://cdnjs.cloudflare.com">
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ==" crossorigin="anonymous" referrerpolicy="no-referrer" media="print" onload="this.media='all'">
    <noscript><link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.3.0/css/all.min.css" integrity="sha512-SzlrxWUlpfuzQ+pcUCosxcglQRNAq/DZjVsC0lE40xsADsfeQoEypE+enwcOiGjk/bSuGGKHEyjSoQ1zVisanQ==" crossorigin="anonymous" referrerpolicy="no-referrer"></noscript>
  {% endif %}
{% endif %}
