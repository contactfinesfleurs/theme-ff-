body{overflow-y:scroll;scroll-behavior:smooth;-ms-overflow-style:none;scrollbar-width:none}
body::-webkit-scrollbar{display:none}
</style>
{% unless request.page_type == 'index' %}
  {{ 'udesly-common.min.css' | asset_url | stylesheet_tag }}
{% endunless %}
