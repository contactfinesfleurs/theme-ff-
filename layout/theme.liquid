  <script>
    (function(){
      function initClock(){
        var tz="{{ settings.timezone }}";
        function u(){
          var n=new Date(),o,d;
          try{o={timeZone:tz,hour:"numeric",minute:"numeric",hour12:true,timeZoneName:"short"};d={timeZone:tz,year:"numeric",month:"2-digit",day:"2-digit"};}
          catch(e){o={hour:"numeric",minute:"numeric",hour12:true,timeZoneName:"short"};d={year:"numeric",month:"2-digit",day:"2-digit"};}
          var t=new Intl.DateTimeFormat("en-US",o).format(n);
          var dd=new Intl.DateTimeFormat("en-US",d).format(n).replace(/(\d+)\/(\d+)\/(\d+)/,'$1/$2/$3');
          document.querySelectorAll(".date").forEach(function(e){e.textContent=dd});
          document.querySelectorAll(".time").forEach(function(e){e.textContent=t});
        }
        u();setInterval(u,60000);
      }
      if('requestIdleCallback' in window){requestIdleCallback(initClock,{timeout:2000})}
      else{setTimeout(initClock,100)}
    })();
  </script>
  {% if request.page_type == 'index' %}
    {% if settings.show_music %}
      <script>
        (function() {
          var musicLoaded = false;

          function loadMusicScript() {
            if (musicLoaded) return;
            musicLoaded = true;
            var script = document.createElement('script');
            script.src = "{{ 'music.js' | asset_url }}";
            script.defer = true;
            document.body.appendChild(script);
          }

          var interactionEvents = ['pointerdown', 'keydown', 'touchstart', 'scroll'];
          interactionEvents.forEach(function(eventName) {
            window.addEventListener(eventName, loadMusicScript, { once: true, passive: true });
          });

          window.addEventListener('load', function() {
            if ('requestIdleCallback' in window) {
              requestIdleCallback(loadMusicScript, { timeout: 5000 });
            } else {
              setTimeout(loadMusicScript, 5000);
            }
          });
        })();
      </script>
    {% endif %}
