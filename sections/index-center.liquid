<div
  class="main-menu"
  style="z-index: 1; height: auto; width: auto; top: {{ section.settings.home_page_menu_positioning }}%;"
  flight="{{ request.page_type }}">
  <div style="width: 300px; left: auto; right: auto; justify-content: center; display: flex;">
    {% if settings.show_home_logo %}
      <a
        href="{{ routes.root_url }}"
        aria-current="page"
        class="logo-link w-inline-block w--current">
        {% if settings.enter_screen_logo %}
          {% assign home_logo_size = settings.enter_screen_logo_size | default: 300 %}
          <img
            src="{{ settings.enter_screen_logo | image_url: width: 800 }}"
            srcset="{{ settings.enter_screen_logo | image_url: width: 320 }} 320w, {{ settings.enter_screen_logo | image_url: width: 480 }} 480w, {{ settings.enter_screen_logo | image_url: width: 640 }} 640w, {{ settings.enter_screen_logo | image_url: width: 800 }} 800w, {{ settings.enter_screen_logo | image_url: width: 1200 }} 1200w"
            sizes="(max-width: 749px) 70vw, {{ home_logo_size }}px"
            width="{{ settings.enter_screen_logo.width }}"
            height="{{ settings.enter_screen_logo.height }}"
            loading="eager"
            decoding="sync"
            fetchpriority="high"
            style="width: {{ home_logo_size }}px;"
            alt="{{ shop.name | escape }}"
            class="logo">
        {% else %}

          <img
            style="width: {{ settings.enter_screen_logo_size }}px;"
            src="{{ 'Supreme-dummy.png' | asset_url }}"
            loading="eager"
            decoding="sync"
            fetchpriority="high"
            alt="{{ shop.name | escape }}"
            class="logo">

        {% endif %}
      </a>
    {% endif %}
  </div>
    {% if section.settings.show_date_time %}
      <div class="date-time-container">
        <div class="date" style="color: {{ section.settings.index_date_color }}">{{ 'tn7d8e379f' | t }}</div>
        <div class="time" style="color: {{ section.settings.index_time_color }}">{{ 't589b2285' | t }}</div>
      </div>
    {% endif %}
  <div class="pages-menu home-menu-grid-mobile">
    {% if section.settings.display_login_link == 'top' %}
      {% if shop.customer_accounts_enabled %}
        {% if customer %}
          <a
            href="{{ routes.account_url }}"
            class="nav-link link-block w-inline-block enter-nav-link"
            style="
                            {% if section.settings['link-alignment'] == 'left' %}
                                    text-align: left;
                            {% else %}
                                    margin-left: auto;
                                    margin-right: auto;
                            {% endif %}
                                    width: fit-content;
                                    height: {{ section.settings.menu-spacing }}px;
                            {% if section.settings.enable_nav_scaling %}
                                    transition: transform 0.3s;
                                    --enter-nav-scaling: scale(1.1);
                            {% endif %}
                            {% if settings.disable_highlight_animation %} animation-duration: 0s; {% endif %}
                            {% if section.settings.disable_highlight_effect %} background-image: none; {% endif %}
                                    font-size:{{ section.settings.font-size }}px;
                                    --navlink-hover-color: {{ section.settings.navlink_hover_color }};
                                    --navlink-background-color: {{ section.settings.navlink_background_color }};
                                    --nav-link-color: {{ section.settings.nav_link_color }};
                                    ">
            {{ 'customer.account_link' | t }}
          </a>
        {% else %}
          <a
            href="{{ routes.account_login_url }}"
            class="nav-link link-block w-inline-block enter-nav-link"
            style="
                            {% if section.settings['link-alignment'] == 'left' %}
                                    text-align: left;
                            {% else %}
                                    margin-left: auto;
                                    margin-right: auto;
                            {% endif %}
                                    width: fit-content;
                                    height: {{ section.settings.menu-spacing }}px;
                            {% if section.settings.enable_nav_scaling %}
                                    transition: transform 0.3s;
                                    --enter-nav-scaling: scale(1.1);
                            {% endif %}
                            {% if settings.disable_highlight_animation %} animation-duration: 0s; {% endif %}
                            {% if section.settings.disable_highlight_effect %} background-image: none; {% endif %}
                                    font-size:{{ section.settings.font-size }}px;
                                    --navlink-hover-color: {{ section.settings.navlink_hover_color }};
                                    --navlink-background-color: {{ section.settings.navlink_background_color }};
                                    --nav-link-color: {{ section.settings.nav_link_color }};
                                    ">
            {{ 'customer.log_in' | t }}
          </a>
        {% endif %}
      {% endif %}
    {% endif %}

    {%- for link in section.settings.center-menu.links -%}
      {% if section.settings['link-alignment'] == 'left' %}
        <a
          href="{{ link.url }}"
          class="nav-link link-block w-inline-block enter-nav-link"
          style="
                                    text-align: left;
                                    width: fit-content;
                                    height: {{ section.settings.menu-spacing }}px;
                            {% if section.settings.enable_nav_scaling %}
                                    transition: transform 0.3s;
                                    --enter-nav-scaling: scale(1.1);
                            {% endif %}
                            {% if settings.disable_highlight_animation %} animation-duration: 0s; {% endif %}
                            {% if section.settings.disable_highlight_effect %} background-image: none; {% endif %}
                                    font-size:{{ section.settings.font-size }}px;
                                    --navlink-hover-color: {{ section.settings.navlink_hover_color }};
                                    --navlink-background-color: {{ section.settings.navlink_background_color }};
                                    --nav-link-color: {{ section.settings.nav_link_color }};
                                    ">
          {{ link.title }}
        </a>
      {% elsif section.settings['link-alignment'] == 'center' %}
        <a
          href="{{ link.url }}"
          class="nav-link link-block w-inline-block enter-nav-link"
          style="
                                    margin-left: auto;
                                    margin-right: auto;
                                    text-align: center;
                                    width: fit-content;
                                    height: {{ section.settings.menu-spacing }}px;
                            {% if section.settings.enable_nav_scaling %}
                                    transition: transform 0.3s;
                                    --enter-nav-scaling: scale(1.1);
                            {% endif %}
                            {% if settings.disable_highlight_animation %} animation-duration: 0s; {% endif %}
                            {% if section.settings.disable_highlight_effect %} background-image: none; {% endif %}
                                    font-size:{{ section.settings.font-size }}px;
                                    --navlink-hover-color: {{ section.settings.navlink_hover_color }};
                                    --navlink-background-color: {{ section.settings.navlink_background_color }};
                                    --nav-link-color: {{ section.settings.nav_link_color }}
                                    ">
          {{ link.title }}
        </a>
      {% endif %}
    {%- endfor -%}
    {% if section.settings.display_login_link == 'bottom' %}
      {% if shop.customer_accounts_enabled %}
        {% if customer %}
          <a
            href="{{ routes.account_url }}"
            class="nav-link link-block w-inline-block enter-nav-link"
            style="
                                {% if section.settings['link-alignment'] == 'left' %}
                                        text-align: left;
                                {% else %}
                                        margin-left: auto;
                                        margin-right: auto;
                                {% endif %}
                                        width: fit-content;
                                        height: {{ section.settings.menu-spacing }}px;
                                {% if section.settings.enable_nav_scaling %}
                                        transition: transform 0.3s;
                                        --enter-nav-scaling: scale(1.1);
                                {% endif %}
                                {% if settings.disable_highlight_animation %} animation-duration: 0s; {% endif %}
                                {% if section.settings.disable_highlight_effect %} background-image: none; {% endif %}
                                        font-size:{{ section.settings.font-size }}px;
                                        --navlink-hover-color: {{ section.settings.navlink_hover_color }};
                                        --navlink-background-color: {{ section.settings.navlink_background_color }};
                                        --nav-link-color: {{ section.settings.nav_link_color }};
                                        ">
            {{ 'customer.account_link' | t }}
          </a>
        {% else %}
          <a
            href="{{ routes.account_login_url }}"
            class="nav-link link-block w-inline-block enter-nav-link"
            style="
                                {% if section.settings['link-alignment'] == 'left' %}
                                        text-align: left;
                                {% else %}
                                        margin-left: auto;
                                        margin-right: auto;
                                {% endif %}
                                        width: fit-content;
                                        height: {{ section.settings.menu-spacing }}px;
                                {% if section.settings.enable_nav_scaling %}
                                        transition: transform 0.3s;
                                        --enter-nav-scaling: scale(1.1);
                                {% endif %}
                                {% if settings.disable_highlight_animation %} animation-duration: 0s; {% endif %}
                                {% if section.settings.disable_highlight_effect %} background-image: none; {% endif %}
                                        font-size:{{ section.settings.font-size }}px;
                                        --navlink-hover-color: {{ section.settings.navlink_hover_color }};
                                        --navlink-background-color: {{ section.settings.navlink_background_color }};
                                        --nav-link-color: {{ section.settings.nav_link_color }};
                                        ">
            {{ 'customer.log_in' | t }}
          </a>
        {% endif %}
      {% endif %}
    {% endif %}
    {%- render 'menu-error'
      , menu: 'main-menu' -%}

